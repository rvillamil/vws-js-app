{
  "name": "vws-js-app",
  "productName": "Video Website Scraper",
  "version": "0.0.5",
  "description": "Electron application for web-scraping on torrent video websites",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=app/assets/icons/mac/icon.icns --no-prune --out=release-builds",
    "package-win": "electron-packager .  --overwrite --asar=true --platform=win32 --arch=ia32 --icon=app/assets/icons/win/icon.ico --no-prune --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Video Website Scraper\"",
    "package-linux": "electron-packager .  --overwrite --asar=true --platform=linux --arch=x64 --icon=app/assets/icons/png/1024x1024.png --no-prune --out=release-builds",
    "package-all": "npm run package-mac && npm run package-win && npm run package-linux"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rvillamil/vws-js-app.git"
  },
  "keywords": [
    "scrapping",
    "torrent",
    "portal",
    "video",
    "Electron"
  ],
  "author": "Rodrigo Villamil PÃ©rez",
  "license": "MIT",
  "homepage": "https://github.com/rvillamil/vws-js-app",
  "devDependencies": {
    "electron": "~1.8.2",
    "electron-packager": "^11.0.1"    
  },

  "dependencies": {
    "nedb":"*",
    "vws-js-app": "git+https://github.com/rvillamil/vws-js-lib.git"
  }
}