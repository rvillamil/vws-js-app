{
  "name": "vws-js-app",
  "productName": "VWS: Video Website Scraper",
  "version": "0.0.1",
  "description": "Electron application for web-scraping on torrent video websites",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"VWS: Video Website Scraper\"",
    "package-linux": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/1024x1024.png --prune=true --out=release-builds"

  },
  "repository": {
    "type": "git",
    "url": "git+https://rvillamil@bitbucket.org/rvillamil/vws-js-app.git"
  },
  "keywords": [
    "scrapping",
    "torrent",
    "portal",
    "video",
    "Electron"
  ],
  "author": "Rodrigo Villamil PÃ©rez",
  "license": "MIT",
  "homepage": "https://bitbucket.org/rvillamil/vws-js-app#readme",
  "devDependencies": {
    "electron": "~1.8.2",
    "electron-packager": "^11.0.1"
  }
}